<?xml version="1.0" encoding="UTF-8"?>

		<sheet type="sequence" name="Operations" link_type="Ope">
		
			<!-- Sheet description. Use &#10; as carriage return -->
			<description>Displays Jeyzer time sequence display features, focusing on operations.&#10;Operation is code executed at low level.</description>
		
			<row_headers>
				<thread_id action_link="true"/>
				<thread_name>
					<highlight name="Demo thread" regex="Demo thread.*" color="RGB-172-186-230"/>  <!-- Light grey blue-->
					<highlight name="Lock feature thread" regex="Lock.*" color="RGB-236-201-166"/>  <!-- Light skin -->
					<highlight name="Deadlock feature thread" regex="Deadlock.*" color="RGB-236-164-152"/>  <!-- Red skin-->
				</thread_name>
			</row_headers>
		
			<display>

				<advanced_operation all_operations="true">
					<highlight name="(OTBI)" color="GREY_25_PERCENT"/>
					<highlight name="Thread on hold (sleep)" color="RGB-119-165-233"/> <!-- Blue light-->
					<highlight name="Memory creation : fill list with random long numbers" color="RGB-254-148-6"/> <!-- Orange light-->
					<highlight name="CPU creation : do mathematical operations" color="RGB-72-181-11"/> <!-- Green light-->
					<highlight name="Method action level2 is synchronized" color="RGB-191-219-157"/> <!-- Green ultra light-->
					<highlight name="Access to already locked resource" color="RGB-255-101-101"/> <!-- Red light-->
					
					<highlight name="Forehand shot" color="RGB-247-225-241"/> <!-- Pink light-->
					<highlight name="Grip shot" color="RGB-242-206-233"/>
					<highlight name="Backhand shot" color="RGB-237-185-223"/>
					<highlight name="Serve shot" color="RGB-231-163-213"/>  <!-- Pink dark-->
				</advanced_operation>

				<!-- Stack trace display as cell comment. Adds overhead processing -->
				<stack/>
				
			</display>
		</sheet>
		
