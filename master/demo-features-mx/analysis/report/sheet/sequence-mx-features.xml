<?xml version="1.0" encoding="UTF-8"?>

		<sheet type="sequence" name="MX Features (Tennis)" link_type="Mxf">
		
			<!-- Sheet description. Use &#10; as carriage return -->
			<description>Displays Jeyzer time sequence display features, focusing on Jeyzer MX features.</description>
		
			<!-- Optional -->
			<headers formats="Advanced JMX,Advanced Agent">
			
				<!-- Functions : average, min, max, cumulative, variance, standard_deviation -->
				<math_functions>
					<max/>
					<average/>
					<cumulative/>
					<standard_deviation/>
				</math_functions>
			
				<!-- Headers -->
				<jzr_mx_context_param name="tennis-score" display="Score" display_comment="Game score"/>
				<jzr_mx_context_param_number name="tennis-serves" display="Serves" display_comment="Cumulative total of serves." threshold="99999" color="RED">
					<!-- Factor defined in percentage gain or loss -->
					<delta increase_factor="20" increase_color="RGB-255-180-129" decrease_factor="20" decrease_color="RGB-167-255-167"/>
				</jzr_mx_context_param_number>
				<jzr_mx_context_param_number name="tennis-balls-played" display="Balls played" display_comment="Cumulative total of played balls." threshold="99999" color="RED">
					<!-- Factor defined in percentage gain or loss -->
					<delta increase_factor="20" increase_color="RGB-255-180-129" decrease_factor="20" decrease_color="RGB-167-255-167"/>
				</jzr_mx_context_param_number>
				<jzr_mx_all_context_params display="All params"/>
				
				<section_delimiter title="Demo MX generic" color="RGB-201-208-168"/>
				<!-- name is regex pattern. Matched attribute should be unique (first match considered) -->
				<mx_bean_param name="org.jeyzer.demo:DemoFeatures:PlayedFeatures" display="Played features" display_comment="Played features"/>
				
				<!--
				<section_delimiter title="Volley ball MX generic" color="RGB-201-208-168"/>
				<mx_bean_param name="org.jeyzer.demo:VolleyBall:Action" display="Volley action" display_comment="Volley action"/>
				-->
				
			</headers>
		
			<row_headers freeze="true">
				<jzr_mx_context_id action_link="true"/>
				<jzr_mx_action_user/>
				<jzr_mx_action_start_time/>
				<jzr_mx_action_function_principal/>
				<jzr_mx_context_param name="level" display="Level" display_comment="Player level"/>
				<jzr_mx_all_context_params display="Ctx params"/>
			</row_headers>
		
			<display>
			
				<!-- Context parameters display as cell comment. -->
				<jzr_mx_all_context_params/>
				<jzr_mx_context_param_number name="tennis-points" threshold="5" color="RGB-198-239-206" />
				
				<!-- Alternative 1 : display serves -->
				<!--jzr_mx_context_param_number name="tennis-serves" threshold="7" color="RGB-198-239-206"/-->
				
				<!-- Alternative 2 : display all values -->
				<!--jzr_mx_context_param name="tennis-serves"/-->
				<!--jzr_mx_context_param name="tennis-points"/-->
				
			</display>
		</sheet>
		
