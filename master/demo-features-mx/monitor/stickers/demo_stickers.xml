<?xml version="1.0" encoding="UTF-8"?>

<stickers group="Demo">

	<!-- Analyzer ambient stickers. Examples : code_quality, performance, QA..  -->
	<!-- Recommended way is to reuse the standard sticker list (defined in the default_setup.xml) 
	     and therefore set the extra stickers onto the JEYZER_MONITOR_ANALYZER_STICKERS variable -->
	<sticker_set list="flights" group="flight"/>

	<sticker name="logback-1.0.x" type="process jar version" jar_name="logback-classic" pattern="^1\.0\.*"/>
	
	<sticker name="logback-1.3.alpha" type="process jar version" jar_name="logback-classic" pattern="^1\.3.*alpha.*"/>
	
	<sticker name="java-base.11.x" type="process module version" module_name="java.base" pattern="^11\.*"/>

	<sticker name="london-airport" type="property card" property="jzr.cxt.param-departure-airport" pattern="London"/>

	<sticker name="demo_unmatched_sticker" type="property card" property="unmatched.property" pattern="unmatched_pattern"/>

</stickers>