<?xml version="1.0" encoding="UTF-8"?>

<multi_monitor parallel_max="1">

	<monitor file="${JEYZER_DEMO_MASTER_PROFILES_DIR}/${JEYZER_TARGET_PROFILE_1}/${JEYZER_TARGET_PROFILE_1}_monitor.xml">
		
		<analysis file="${JEYZER_DEMO_MASTER_PROFILES_DIR}/${JEYZER_TARGET_PROFILE_1}/${JEYZER_TARGET_PROFILE_1}_analysis.xml"/>
		
		<environment>
			<param name="JEYZER_TARGET_PROFILE" value="${JEYZER_TARGET_PROFILE_1}"/>
			<param name="JEYZER_TARGET_NAME" value="${JEYZER_TARGET_PROFILE_1}_node_1"/>
			
			<!-- Directories -->
			<param name="JEYZER_RECORD_DIRECTORY" value="${JEYZER_RECORDINGS_ROOT_DIR}/${JEYZER_TARGET_PROFILE_1}"/>
			<param name="JEYZER_OUTPUT_DIR" value="${JEYZER_RECORDINGS_ROOT_DIR}/${JEYZER_TARGET_PROFILE_1}"/>
			
			<!-- Periods -->
			<param name="JEYZER_RECORD_PERIOD" value="5s"/>
			<!-- Make sure scanning period is lower than the JEYZER_RECORD_ARCHIVE_FILE_LIMIT -->
			<param name="JEYZER_MONITOR_SCAN_PERIOD" value="2m"/>
			
			<!-- Report generation and dispatching -->
			<param name="JEYZER_JZR_REPORT_ENABLED" value="true"/>
			<param name="JEYZER_MONITOR_ALERT_EMAIL_ENABLED" value="false"/>
			<param name="JEYZER_MONITOR_GRAPH_PICTURE_ENABLED" value="true"/>
			<param name="JEYZER_MONITOR_SOUND_ENABLED" value="false"/>
			<param name="JEYZER_TARGET_DESCRIPTION" value="Generated by Jeyzer Monitor"/>
			<param name="JEYZER_TARGET_ISSUER" value="Jeyzer Multi-Monitor"/>
			<param name="JEYZER_MONITOR_ANALYZER_STICKERS" value="code_quality, performance"/>
			<param name="JEYZER_MONITOR_WEB_ENABLED" value="${JEYZER_MONITOR_WEB_ENABLED}"/>
			<param name="JEYZER_MONITOR_WEB_HOST" value="${JEYZER_MONITOR_WEB_HOST}"/>
			<param name="JEYZER_MONITOR_WEB_PORT" value="${JEYZER_MONITOR_WEB_PORT}"/>
			<param name="JEYZER_MONITOR_WEB_DEPLOY_DIR" value="${JEYZER_MONITOR_WEB_DEPLOY_DIR}/${JEYZER_TARGET_PROFILE_1}"/>

			<!-- Deobfuscation -->
			<param name="JEYZER_ANALYZER_KEEP_DEOBFUSCATED_FILES" value="true"/>
			<param name="JEYZER_DEOBSFUCATION_CONFIG_DIR" value="${JEYZER_DEOBSFUCATION_CONFIG_DIR}"/>
			
			<!-- Alternate, do not edit -->
			<param name="JEYZER_ANALYZER_REPLAY_ENABLED" value="false"/>
			<param name="JEYZER_ANALYZER_GRAPH_VIEWER_ENABLED" value="false"/>
			<param name="JEYZER_GRAPH_REFRESH_PERIOD" value="3s"/>
			<param name="JEYZER_ANALYZER_TEAM_EMAIL_ENABLED" value="false"/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_1" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_2" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_3" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_4" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_5" value=""/>
		</environment>
		
	</monitor>
	
