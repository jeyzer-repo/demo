			<param name="JEYZER_GRAPH_REFRESH_PERIOD" value="3s"/>
			<param name="JEYZER_ANALYZER_TEAM_EMAIL_ENABLED" value="false"/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_1" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_2" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_3" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_4" value=""/>
			<param name="JEYZER_ANALYZER_DISCOVERY_FUNCTIONS_5" value=""/>
		</environment>
		
	</monitor>

</multi_monitor>
