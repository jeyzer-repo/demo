<?xml version="1.0" encoding="UTF-8"?>

		<sheet type="sequence" name="Philosopher interactions" link_type="Phi">
		
			<!-- Sheet description. Use &#10; as carriage return -->
			<description>Displays all the philosopher interactions.</description>
		
			<row_headers>
				<thread_id action_link="true"/>
				<thread_name/>
				<action_size color="RGB-231-213-137" threshold="10"/> <!-- Light brown -->
				<executor>
					<highlight name="Philosopher" color="RGB-172-186-230"/>  <!-- Light grey blue-->
				</executor>
			</row_headers>
		
			<display>

				<!-- Function display. Color applies for ATBI -->
				<function color="GREY_25_PERCENT"/>

				<advanced_operation all_operations="true">
					<highlight name="(OTBI)" color="GREY_25_PERCENT"/>
					<highlight name="Takes place" color="RGB-119-165-233"/> <!-- Blue light-->
					<highlight name="Waits for first bowl" regex="Waits for first.*" color="RGB-254-148-6"/>  <!-- Orange light-->
					<highlight name="Waits for second bowl" regex="Waits for second.*" color="RGB-255-102-0"/>  
					<highlight name="Eats" color="RGB-72-181-11"/>  <!-- Green light-->
					<highlight name="Sets the table" color="RGB-191-219-157"/>  <!-- Green ultra light-->
					<highlight name="Serves drinks" color="RGB-191-219-157"/>  <!-- Green ultra light-->
				</advanced_operation>

				<!-- Stack trace display as cell comment. Adds overhead processing -->
				<stack/>

			</display>
		</sheet>
		
